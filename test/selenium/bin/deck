#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

export HOSTNAME=$(hostname)

(
    cd $DIR/..

    if [[ ! -d ./test_out ]]; then
        mkdir -p ./test_out/images
        mkdir -p ./test_out/junit
    fi

    node_modules/.bin/ts-node -r tsconfig-paths/register ./src/cli.ts "${@}"
)